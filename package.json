{
  "name": "errore",
  "version": "0.9.0",
  "type": "module",
  "description": "Type-safe errors as values for TypeScript. Like Go, but with full type inference.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remorses/errore.git"
  },
  "homepage": "https://github.com/remorses/errore#readme",
  "bugs": {
    "url": "https://github.com/remorses/errore/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "worker:dev": "wrangler dev",
    "worker:deploy": "wrangler deploy"
  },
  "keywords": [
    "error",
    "result",
    "typescript",
    "type-safe",
    "go",
    "errors-as-values"
  ],
  "author": "remorses",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  }
}
